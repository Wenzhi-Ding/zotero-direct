{
	"name": "bibnotes_minimal",
	"version": "0.10.0",
	"description": "Plugin to export and format annotations from Zotero into Obsidian",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ."
	},
	"keywords": ["Zotero"],
	"author": "Stefano Pagliari; Wenzhi Ding",
	"license": "MIT",
	"devDependencies": {
		"@types/better-sqlite3": "^7.5.0",
		"@types/node": "^22.0.0",
		"builtin-modules": "^4.0.0",
		"esbuild": "^0.25.0",
		"eslint": "^9.0.0",
		"eslint-plugin-obsidianmd": "latest",
		"globals": "^15.0.0",
		"obsidian": "^1.7.2",
		"tslib": "^2.8.0",
		"typescript": "^5.7.0",
		"typescript-eslint": "^8.0.0",
		"@eslint/js": "^9.30.1"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.2",
		"color-classifier": "^0.0.1",
		"debugout.js": "^1.0.0",
		"loglevel": "^1.8.0",
		"turndown": "^7.1.1"
	}
}